{
  "connection": "default",
  "collectionName": "departments",
  "info": {
    "name": "Department"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "links": {
      "type": "json"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
