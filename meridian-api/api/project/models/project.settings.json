{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "active": {
      "type": "boolean"
    },
    "published": {
      "type": "boolean"
    },
    "descriptions": {
      "type": "json"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "team": {
      "model": "team"
    },
    "program": {
      "model": "program"
    },
    "course": {
      "model": "course"
    }
  }
}
